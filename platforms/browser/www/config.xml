<?xml version='1.0' encoding='utf-8'?>
<widget id="com.kerryexpress.kesmobile" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>KesMobile</name>
    <description>
        A tool for pickup and delivery Kerry Express VN.
    </description>
    <author email="thinh.nguyen@kerryexpres.com.vn" href="https://kes.kerryexpress.com.vn">
        IT Kerry Express VN Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="SplashScreenDelay" value="3000" />
</widget>
